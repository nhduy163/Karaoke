	<queryString>
		<![CDATA[SELECT *
from hoadon hd
inner join nhanvien nv on hd.MaNV = nv.MaNV
inner join thuephong tp on tp.MaTP = hd.MaTP
inner join phong p on p.MaPhong = tp.MaPhong
inner join khachhang kh on kh.MaKH = tp.MaKH
left join goidichvu gdv on gdv.MaTP = tp.MaTP
left join dichvu dv on gdv.MaDV = dv.MaDV
where hd.MaHD = 25]]>
	</queryString>
